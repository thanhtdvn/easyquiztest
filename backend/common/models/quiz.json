        "quizName": "string"
      }
    },
    "quizStatus": {
      "type": "string"
    },
    "totalTime": {
      "type": "number"
    },
    "numberOfQuestions": {
      "type": "number"
    },
    "totalQuestions": {
      "type": "number"
    },
    "listQuestionIds": {
      "type": [
        {
          "questionId": "number"
        }
      ]
    },
    "created": {
      "type": "date"
    },
    "createdUserId": {
      "type": "number"
